{
    "name": "scorp/http-framework",
    "description": "source code",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
      "sort-packages": true
    },
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App",
            "Framework\\": "src/Framework"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\App\\": "tests/App",
            "Test\\Framework\\": "tests/Framework"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.29"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
